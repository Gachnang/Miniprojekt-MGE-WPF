<Application x:Class="ch.hsr.wpf.gadgeothek.admin.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
             xmlns:local="clr-namespace:ch.hsr.wpf.gadgeothek.admin"
             StartupUri="MainWindow.xaml">
    <Application.Resources>
        <ResourceDictionary>
            <ResourceDictionary.MergedDictionaries>
                <ResourceDictionary Source = "Theme/Resources.xaml"/>
                <ResourceDictionary Source = "Theme/Shadow/Shadows.xaml"/>
                <ResourceDictionary Source = "Theme/System.Windows.xaml"/>
                <ResourceDictionary Source = "Theme/System.Windows.Control.xaml"/>
                <ResourceDictionary Source = "Theme/System.Windows.Button.xaml"/>
                <ResourceDictionary Source = "Theme/System.Windows.TextBox.xaml"/>
            </ResourceDictionary.MergedDictionaries>
        </ResourceDictionary>
    </Application.Resources>
</Application>
